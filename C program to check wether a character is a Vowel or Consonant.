
#include<stdio.h>
int main(){
char c;
printf("Enter a character: ");
scanf("%c", &c);

switch(c){
case 'a':
printf("%c is a vowel.");
break;
case 'e':
printf("%c is a vowel.");

break;

case 'i':
printf("%c is a vowel.");
break;

case 'o':
printf("%c is a vowel.");
break;

case 'u':


printf("%c is a vowel.");
break;

case 'A':
printf("%c is a vowel.");
break;

case 'E':
printf("%c is a vowel.");
break;

case 'I':
printf("%c is a vowel.");
break;

case 'O':
printf("%c is a vowel.");
break;

case 'U':
printf("%c is a vowel.");

break;
default:
printf("%c is a consonant");
}
return 0;
}






