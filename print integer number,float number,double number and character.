#include <stdio.h>

int main()
{
    int number;
    float number1;
    double number2;
    char a;
    printf("Enter an integer: ");
    scanf("%d", &number);
    printf("Enter a float number: ");
    scanf("%f", &number1);
    printf("Enter a double number: ");
    scanf("%lf", &number2);
    printf("Enter a char a: ");
    scanf("%ch", &a);

    return 0;
}
